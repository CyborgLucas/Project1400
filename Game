def Sarah():
	if sarahp == true and end != true:
		print("Sarah - I have some pieces. You'll need to find the rest of the puzzle.")
	elif sarahp == false and end != true:
		print("Sarah - You have all that I found.")
	else:
		if end == true:
			print("Sarah - Here is the treasure!")
		elif traitor == true and lose == true:
			print("Sarah - Did he get away?")

def Daniel():
	if end == false:
		print("Daniel - Can you open the vault?")
	elif end == true:
		if traitor == true and dendt == 1:
			print("Daniel - YES! You got the clues!")
			dendt += 1
		elif traitor == true and dendt == 2:
			print("Daniel - Oh that THIEF!")
		elif traitor == true and lose == true:
			print("Daniel - ...")
	return dendt

def Fred():
	if fredp == true and end != true:
		print("Fred - I have some pieces to the puzzle.")
	elif fredp == false and end != true:
		print("Fred - I have nothing.")
	elif end == true:
		if traitor == true:
			print("Fred - You will never stop me!")
		elif traitor != true:
			print("Now I can retire!")

